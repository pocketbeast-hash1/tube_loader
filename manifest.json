{
    "manifest_version": 3,
    "name": "Tube Loader",
    "description": "Download videos from Rutube, YouTube, etc.",
    "version": "0.0.1",
    "icons": {},
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ]
}